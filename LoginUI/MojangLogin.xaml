<Page x:Class="Cube_World_Launcher.LoginUI.MojangLogin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cube_World_Launcher.LoginUI"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="200"
      Title="MojangLogin">

    <Grid>
        <TextBox x:Name="Email" HorizontalAlignment="Center" TextWrapping="Wrap" Text="" VerticalAlignment="Center" Width="120" FontSize="16"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Center"  Width="120" FontSize="16" Margin="0,148,0,10" Height="22.32" VerticalAlignment="Center"/>
        <Label Content="账号名称" HorizontalAlignment="Center" Margin="0,54,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Label Content="账号密码" HorizontalAlignment="Center" Margin="0,118,0,0" VerticalAlignment="Top" FontSize="16"/>
    </Grid>
</Page>
